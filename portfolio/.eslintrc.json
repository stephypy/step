{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
    "browser": true,
    "es6": true
  },
    "extends": ["eslint:recommended", "google"],
    "rules": {
	"require-jsdoc": "off",
	"valid-jsdoc": "off",
	"object-curly-spacing": "off",
	"max-len": "off",
	"comma-dangle": "off",
	"space-before-function-paren"
	"indent": "off"  
  }
}
